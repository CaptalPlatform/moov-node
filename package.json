{
    "name": "@moovio/node",
    "version": "0.1.1",
    "description": "Node SDK for the Moov API and Dashboard",
    "main": "lib/index.js",
    "type": "module",
    "engines": {
        "node": ">=14"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/moovfinancial/moov-node"
    },
    "keywords": [
        "nodejs",
        "sdk"
    ],
    "author": {
        "name": "Moov Financial",
        "email": "support@moov.io"
    },
    "license": "Apache-2.0",
    "bugs": {
        "url": "https://github.com/moovfinancial/moov-node/issues"
    },
    "homepage": "https://github.com/moovfinancial/moov-node#readme",
    "files": [
        "lib/**/*"
    ],
    "scripts": {
        "docs": "jsdoc -c ./jsdoc.json -d ./docs -r ./lib",
        "docs-watch": "watch \"npm run docs\" ./lib",
        "docs-clean": "rm -rf ./docs",
        "test": "ava ./lib/**/*.test.js",
        "test-watch": "ava --watch ./lib/**/*.test.js",
        "publish": "npm publish --access public"
    },
    "devDependencies": {
        "ava": "^4.0.0",
        "jsdoc": "^3.6.7",
        "prettier": "^2.5.1",
        "typescript": "^4.5.2",
        "watch": "^0.13.0"
    },
    "dependencies": {
        "got": "^12.0.0"
    }
}
